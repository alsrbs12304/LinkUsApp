<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/createGrp1Fragment">

    <fragment
        android:id="@+id/createGrp1Fragment"
        android:name="com.example.linkusapp.view.fragment.CreateGrp1Fragment"
        android:label="fragment_create_grp1"
        tools:layout="@layout/fragment_create_grp1" >
        <action
            android:id="@+id/action_createGrp1Fragment_to_createGrp2Fragment"
            app:destination="@id/createGrp2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/createGrp2Fragment"
        android:name="com.example.linkusapp.view.fragment.CreateGrp2Fragment"
        android:label="fragment_create_grp2"
        tools:layout="@layout/fragment_create_grp2" >
        <action
            android:id="@+id/action_createGrp2Fragment_to_createGrp1Fragment"
            app:destination="@id/createGrp1Fragment" />
    </fragment>
</navigation>